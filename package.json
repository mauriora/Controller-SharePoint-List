{
    "name": "@mauriora/controller-sharepoint-list",
    "version": "0.2.6",
    "repository": {
        "type": "git",
        "url": "https://github.com/mauriora/Controller-SharePoint-List.git"
    },
    "license": "MIT",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "engines": {
        "node": ">=14.16.0"
    },
    "scripts": {
        "lint": "eslint .",
        "build": "eslint . && tsc --build",
        "rebuild": "eslint . && tsc --build --clean && tsc --build",
        "clean": "tsc --build --clean",
        "release": "yarn run dotenv yarn run semantic-release --dry-run",
        "semantic-release": "semantic-release"
    },
    "release": {
        "branches": [
            "main",
            {
                "name": "build-n-publish", 
                "prerelease": "true"
            }
        ]
    },
    "dependencies": {
        "@microsoft/sp-http": "1.14.0",
        "@pnp/graph": "3.4.0",
        "@pnp/logging": "3.4.0",
        "@pnp/sp": "3.4.0",
        "class-transformer": "^0.5.1",
        "mobx": "^6.6.0",
        "reflect-metadata": "^0.1.13"
    },
    "devDependencies": {
        "@mauriora/eslint-config-ts": "0.0.1",
        "@mauriora/package-tools": "^0.1.2",
        "@microsoft/microsoft-graph-types": "^2.20.0",
        "@microsoft/sp-component-base": "1.14.0",
        "@pnp/spfx-controls-react": "3.8.0",
        "@types/es6-promise": "^3.3.0",
        "@types/webpack-env": "1.17.0",
        "@typescript-eslint/eslint-plugin": "^5.27.0",
        "@typescript-eslint/parser": "^5.27.0",
        "@typescript-eslint/typescript-estree": "^5.27.0",
        "dotenv": "^16.0.1",
        "dotenv-cli": "^5.1.0",
        "eslint": "^7.32.0",
        "semantic-release": "^19.0.2",
        "typescript": "~4.5.5"
    }
}